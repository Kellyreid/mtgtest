<p id="notice"><%= notice %></p>

<h1>Listing Transactions</h1>

<table>
  <thead>
    <tr>
      <th>Partner</th>
      <th>Asset</th>
      <th>Volume</th>
      <th>Price</th>
      <th>Channel</th>
      <th>Notes</th>
      <th>Type</th>
      <th>Partner trade</th>
      <th>Currency</th>
      <th>Owner</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @transactions.each do |transaction| %>
      <tr>
        <td><%= transaction.partner %></td>
        <td><%= transaction.asset_id %></td>
        <td><%= transaction.volume %></td>
        <td><%= transaction.price %></td>
        <td><%= transaction.channel %></td>
        <td><%= transaction.notes %></td>
        <td><%= transaction.type %></td>
        <td><%= transaction.partner_trade_id %></td>
        <td><%= transaction.currency %></td>
        <td><%= transaction.owner_id %></td>
        <td class='btn btn-default'><%= link_to 'Show', transaction %></td>
        <td class='btn btn-warning'><%= link_to 'Edit', edit_transaction_path(transaction) %></td>
        <td class='btn btn-danger'><%= link_to 'Destroy', transaction, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>


<button class='btn btn-success'><i class="glyphicon glyphicon-plus"></i>&nbsp<%= link_to 'New Transaction', new_transaction_path %></button>
